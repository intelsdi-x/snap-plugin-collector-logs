{
    "version": 1,
    "schedule": {
        "type": "simple",
        "interval": "3s"
    },
    "workflow": {
        "collect": {
            "metrics": {
                "/intel/logs/*": {}
            },
            "config": {
                "/intel/logs": {
                    "metric_name": "rabbit_logs",
                    "cache_dir": "/home/test/cache/snap",
                    "log_dir": "/home/test/kolla/*",
                    "log_file": ".*rabbit.*",
                    "log_type": "rabbit"
                }
            }
        }
    }
}
